@model ClinicEntity.Models.OtherStaff
@{
    ViewData["Title"] = "DeleteOtherStaff";
}
@{
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<div style="background-color:#f0f8ff;padding:55px; font-family:'Roboto', sans-serif">
    <form method="post" enctype="multipart/form-data">
        <h3 style="color:#003399" class="font-weight-bolder text-uppercase" align="center">Delete Other Staff Details</h3>

        <div class="form-group">
            <label class="font-weight-bold" style="color:#003399;font-size:20px">Staff Id</label>
            <input type="text" asp-for="StaffID" class="form-control" placeholder="Enter Staff Id" required />
        </div>
        <input type="submit" value="Delete" class="btn btn-outline-danger" />
  
        @if (ViewBag.status == "Ok")
        {
            <br />

            <br />
            <div class="alert alert-warning">
                @ViewBag.message
            </div>
        }
        else if (ViewBag.status == "Error")
        {
            <br />

            <br />
            <div class="alert alert-danger">
                @ViewBag.message
            </div>
        }
    </form>
</div>

